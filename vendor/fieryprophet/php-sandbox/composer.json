{
    "name": "fieryprophet/php-sandbox",
    "description": "A PHP library that can be used to run PHP code in a sandbox environment",
    "keywords": ["php", "sandbox", "parser", "whitelist", "blacklist"],
    "type": "library",
    "homepage": "http://www.fieryprophet.com/phpsandbox",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Elijah Horton"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fieryprophet/php-sandbox"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "nikic/php-parser": "dev-master",
        "jeremeamia/functionparser": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpdocumentor/phpdocumentor": "dev-master"
    },
    "autoload": {
        "psr-0": { "PHPSandbox": "src/" }
    }
}